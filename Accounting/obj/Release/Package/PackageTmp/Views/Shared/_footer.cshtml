<script type="text/javascript">



        $(document).ready(function () {

             if ('@ViewBag.res' == 1) {

                 swal("İşlem Başarısız", "İşlem Gerçekleştirilimedi!", "error");

             }
             else if ('@ViewBag.res' == 2) {

                 swal("İşlem Başarısız", "Dosya Yükleyiniz!", "warning");

             }
             else if ('@ViewBag.res' == 3) {

                 swal("İşlem Başarısız", "Excel Dosyası Yükleyiniz!", "warning");

             }
             else if ('@ViewBag.res' == 4) {

                 swal("İşlem Başarılı", "Mutabakat Oluşturuldu", "success");

             }
             else if ('@ViewBag.res' == 5) {

                swal("Mutabakat Oluşturulamadı!", "Sistem de Aynı İsimle Kayıtlı Excel Dosyası Bulunmaktadır Veya Geçersiz Excel Dosyası girişi yaptınız!!!", "error");

             }
             else if ('@ViewBag.res' == 6) {

                swal("İşlem Başarısız!", "Boşlukları Doldurunuz!", "error");

             }
        });



</script>
